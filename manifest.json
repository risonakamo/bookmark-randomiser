{
    "name":"Bookmark Randomiser",
    "description":"Randomise bookmarks in folders",
    "version":"0.0.1",
    "version_name":"0.0.1-beta",
    "homepage_url":"https://github.com/risonakamo/bookmark-randomiser",
    "manifest_version":3,

    "permissions":[
        "favicon",
        "bookmarks",
        "tabs"
    ],

    "action":{
        "default_title":"Bookmark Randomiser",
        "default_popup":"build/pages/popup/index.html"
    }
}